	#loginbtn {
	color:White;
	background:#4490f7;
	text-decoration:none;
	padding:10px 95px;
	margin-left:87px;
	margin-top:40px;
	border-radius:5px;
	opacity:0.8;
	filter:alpha(opacity=80);
}

