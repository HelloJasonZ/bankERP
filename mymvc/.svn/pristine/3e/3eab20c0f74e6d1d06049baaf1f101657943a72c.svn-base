@charset "utf-8";
	.mask {
	margin:0;
	padding:0;
	border:none;
	width:100%;
	height:100%;
	background:#333;
	opacity:0.6;
	filter:alpha(opacity=60);
	z-index:9999;
	position:fixed;
	top:0;
	left:0;
	display:none;
}
#LoginBox {
	position:absolute;
	/* left:460px;
	top:150px;*/
	background:white;
	width:426px;
	height:170px;
	border:3px solid #444;
	border-radius:7px;
	z-index:10000;
	display:none;
}
