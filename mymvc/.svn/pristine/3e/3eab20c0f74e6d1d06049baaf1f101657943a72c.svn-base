} */
.close_btn {
	font-family:arial;
	font-size:30px;
	font-weight:700;
	color:#999;
	text-decoration:none;
	float:right;
	padding-right:4px;
}
.inputBox {
	border:1px solid #c3c3c3;
	padding:1px 3px 6px 3px;
	border-radius:5px;
	margin-left:5px;
}
